pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello, World!'
            }
        }
        stage('Welcome to Devops') {
            steps {
                echo 'Hello, Devops'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hello, Hi'
            }
        }
    }
}
