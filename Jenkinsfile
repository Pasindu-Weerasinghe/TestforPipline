pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello, World! 1'
                echo 'Hello, World! 2'
                echo 'Hello, World! 3'
                echo 'Hello, World! 4'
                echo 'Hello, World! 5'
            }
        }
        stage('Welcome to Devops') {
            steps {
                echo 'Hello, Devops 1'
                echo 'Hello, Devops 2'
                echo 'Hello, Devops 3'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hello, Hi 1'
                echo 'Hello, Hi 2'
            }
        }
    }
}
